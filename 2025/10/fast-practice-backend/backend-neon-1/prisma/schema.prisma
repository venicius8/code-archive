generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum MaleOrFemale {
  Male
  Female
}

model Pet {
  id  Int  @id @default(autoincrement())
  name String
  gender MaleOrFemale
  race String
  age Int
  createdAt DateTime @default(now())
}
